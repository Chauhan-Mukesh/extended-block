parameters:
    level: 6
    paths:
        - src
    excludePaths:
        - src/Resources/public/*
        # Studio UI components - interface only available when Studio UI Bundle is installed
        - src/Webpack/*
    ignoreErrors:
        # Pimcore classes may not be available during static analysis
        -
            message: '#Class Pimcore\\.*not found#'
            reportUnmatched: false
        -
            message: '#Call to method.*on an unknown class Pimcore\\.*#'
            reportUnmatched: false
        # __set_state is a standard PHP pattern for var_export serialization
        # The static constructor is safe here because ExtendedBlock is a final-like class
        -
            message: '#Unsafe usage of new static\(\)#'
            path: src/Model/DataObject/ClassDefinition/Data/ExtendedBlock.php
            count: 1
